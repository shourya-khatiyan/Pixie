

# env
ENVIRONMENT=development
LOG_LEVEL=DEBUG


OPENAI_API_KEY=sk-your-openai-api-key-here

ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# vector db(Qdrant)
QDRANT_HOST=localhost
QDRANT_PORT=6333
# api key only needed for Qdrant cloud
QDRANT_API_KEY=


# postgreSQL (read-only access)
# Format: postgresql+asyncpg://user:password@host:port/database
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/pixie

# for caching
REDIS_URL=redis://localhost:6379/0


PORT=8000
# Comma-separated list of allowed CORS origins
ALLOWED_ORIGINS=http://localhost:3000


# Internal API Key
# Shared secret for Node.js backend communication
# Generate a secure random string for production
INTERNAL_API_KEY=dev-internal-key-change-in-production
